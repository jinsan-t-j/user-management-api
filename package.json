    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsdoc": "^50.0.0",
    "joi": "^17.13.3",
    "nanoid": "^3.3.4",
    "nest-winston": "^1.10.0",
    "nestjs-i18n": "^10.4.5",
